.at-tag {
  display: inline-block;
  padding: 0 32rpx;
  height: 60rpx;
  color: #666;
  font-size: 28rpx;
  text-align: center;
  line-height: 58rpx;
  vertical-align: middle;
  border: 1PX solid #F7F7F7;
  border-radius: 8rpx;
  background-color: #F7F7F7;
  box-sizing: border-box;
  /* modifiers */
}
.at-tag--primary {
  color: #666;
  border-color: #F7F7F7;
  background-color: #F7F7F7;
}
.at-tag--active {
  color: #78A4F4;
  border-color: #78A4F4;
  background-color: #FFF;
}
.at-tag--active.at-tag--primary {
  color: #FFF;
  border-color: #78A4F4;
  background-color: #78A4F4;
}
.at-tag--disabled {
  opacity: 0.3;
}
.at-tag--circle {
  border-radius: 30rpx;
  background-clip: border-box;
  overflow: hidden;
}
.at-tag--small {
  padding: 0 16rpx;
  height: 40rpx;
  font-size: 20rpx;
  line-height: 38rpx;
}
.at-tag--small.at-tag--circle {
  border-radius: 20rpx;
}
