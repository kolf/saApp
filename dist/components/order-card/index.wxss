.at-card {
  border-top: 1PX #d6e4ef solid;
  border-bottom: 1PX #d6e4ef solid;
  border-right: 1PX #d6e4ef solid;
  border-left: 1PX #d6e4ef solid;
  margin: 0 24rpx;
  border-radius: 12rpx;
  background-color: #FFF;
  /* elements */
  /* modifiers */
}
.at-card__header {
  display: flex;
  align-items: center;
  -webkit-box-align: center;
  border-bottom: 1PX #d6e4ef solid;
  padding: 18rpx 24rpx;
  position: relative;
}
.at-card__header-title, .at-card__header-extra {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 1.5;
}
.at-card__header-thumb {
  flex: 0 0 32rpx;
  -webkit-box-flex: 0;
  height: 32rpx;
  display: block;
  line-height: 0;
  margin-right: 16rpx;
}
.at-card__header-thumb-info {
  width: 100%;
  height: 100%;
}
.at-card__header-icon {
  margin-right: 16rpx;
  font-size: 32rpx;
}
.at-card__header-title {
  flex: auto;
  -webkit-box-flex: auto;
  display: block;
  color: #2C405A;
  font-size: 32rpx;
}
.at-card__header-extra {
  flex: 0 0 auto;
  -webkit-box-flex: 0;
  max-width: 200rpx;
  margin-left: 24rpx;
  color: #2C405A;
  font-size: 28rpx;
}
.at-card__content {
  padding: 24rpx 24rpx;
}
.at-card__content-info {
  color: #333;
  font-size: 28rpx;
}
.at-card__content-note {
  margin-top: 18rpx;
  color: #999;
  font-size: 24rpx;
}
.at-card--full {
  margin: 0;
  border-radius: 0;
  border-left: none;
  border-right: none;
}
