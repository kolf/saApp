<template name="renderMain">
    <block>
        <block wx:if="{{isFetching}}">
            <at-activity-indicator compid="{{$compid__426}}"></at-activity-indicator>
        </block>
        <block wx:elif="{{showType === 0}}">
            <view class="report__main">
                <view style="width:100%;height:320px">
                    <f-2-canvas compid="{{$compid__427}}"></f-2-canvas>
                </view>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-2">顾问</view>
                        <view class="at-col at-col-3">二手车评估</view>
                        <view class="at-col at-col-2">再购新车</view>
                        <view class="at-col at-col-2">转介绍</view>
                        <view class="at-col at-col-3">推荐总量</view>
                    </view>
                    <view class="table-body">
                        <block wx:if="{{data}}">
                            <view class="at-row at-list__item" wx:key="name" wx:for="{{$anonymousCallee__19}}" wx:for-item="item" wx:for-index="_anonIdx2">
                                <view class="at-col at-col-2">{{item.name}}</view>
                                <view class="at-col at-col-3">{{item.escpgCount}}</view>
                                <view class="at-col at-col-2">{{item.zgxcCount}}</view>
                                <view class="at-col at-col-2">{{item.zjsCount}}</view>
                                <view class="at-col at-col-3">{{item.total}}</view>
                            </view>
                        </block>
                    </view>
                </view>
            </view>
        </block>
        <block wx:elif="{{showType === 1}}">
            <view class="report__main">
                <view style="width:100%;height:320px">
                    <f-2-canvas compid="{{$compid__428}}"></f-2-canvas>
                </view>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-3">日期</view>
                        <view class="at-col at-col-3" wx:for="{{zheData}}" wx:for-item="item" wx:for-index="_anonIdx4">{{item.realName}}</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item" wx:key="orderDate" wx:for="{{$anonymousCallee__21}}" wx:for-item="item" wx:for-index="_anonIdx8">
                            <view class="at-col at-col-3">{{item.orderDate}}</view>
                            <view class="at-col at-col-3" wx:for="{{zheData}}" wx:for-item="val" wx:for-index="_anonIdx6">{{item[val.escUserId] || 0}}</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
    </block>
</template>
<block wx:if="{{$taroCompReady}}">
    <view class="page report__root">
        <at-tabs compid="{{$compid__429}}"></at-tabs>
        <view class="report__control-wrap">
            <at-segmented-control compid="{{$compid__430}}"></at-segmented-control>
        </view>
        <view class="border-bottom report__pager">
            <pager compid="{{$compid__431}}"></pager>
        </view>
        <template is="renderMain" data="{{...anonymousState__temp}}"></template>
    </view>
</block>