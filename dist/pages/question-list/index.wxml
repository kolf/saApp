<block wx:if="{{$taroCompReady}}">
    <view class="page bg-gray question-list__root">
        <view class="content">
            <block>
                <block wx:if="{{listData.length > 0}}">
                    <view class="question-list__list">
                        <view class="at-card question-list__item" wx:for="{{listData}}" wx:for-item="item" wx:for-index="_anonIdx2">
                            <view class="at-card__header"><text class="taro-text at-card__header-title question-list__item-date">{{item.feedbackTime}}</text>
                                <view class="taro-text at-card__header-extra question-list__item-extra">{{item.typeName}} {{item.subTypeName}}</view>
                            </view>
                            <view class="at-card__content">
                                <view class="question-list__item-title">问题描述：</view>
                                <view class="question-list__item-desc">{{item.feedbackContent}}</view>
                            </view>
                        </view>
                    </view>
                </block>
                <block wx:else>
                    <empty-data compid="{{$compid__445}}">您还没有发起过任何问题反馈</empty-data>
                </block>
            </block>
        </view>
        <view class="footer-fixed__buttons">
            <at-button compid="{{$compid__446}}">问题反馈</at-button>
        </view>
    </view>
</block>