<template name="renderMain">
    <block>
        <block wx:if="{{isFetching}}">
            <at-activity-indicator compid="{{$compid__432}}"></at-activity-indicator>
        </block>
        <block wx:elif="{{showType === 0}}">
            <view class="report__main">
                <view style="width:100%;height:320px">
                    <f-2-canvas compid="{{$compid__433}}"></f-2-canvas>
                </view>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-1">顾问</view>
                        <view class="at-col at-col-4">仅评估台数</view>
                        <view class="at-col at-col-4">推荐置换台数</view>
                        <view class="at-col at-col-3">完成总台数</view>
                    </view>
                    <view class="table-body">
                        <block wx:if="{{data}}">
                            <view class="at-row at-list__item" wx:key="name" wx:for="{{$anonymousCallee__22}}" wx:for-item="item" wx:for-index="_anonIdx2">
                                <view class="at-col at-col-3">{{item.name}}</view>
                                <view class="at-col at-col-3">{{item.escpgTotal}}</view>
                                <view class="at-col at-col-3">{{item.escpgTotal}}</view>
                                <view class="at-col at-col-3">{{item.total}}</view>
                            </view>
                        </block>
                    </view>
                </view>
            </view>
        </block>
        <block wx:elif="{{showType === 1}}">
            <view class="report__main">
                <view style="width:100%;height:320px">
                    <f-2-canvas compid="{{$compid__434}}"></f-2-canvas>
                </view>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-3">日期</view>
                        <view class="at-col at-col-3" wx:for="{{zheData}}" wx:for-item="item" wx:for-index="_anonIdx4">{{item.realName}}</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item" wx:key="orderDate" wx:for="{{$anonymousCallee__24}}" wx:for-item="item" wx:for-index="_anonIdx8">
                            <view class="at-col at-col-3">{{item.orderDate}}</view>
                            <view class="at-col at-col-3" wx:for="{{zheData}}" wx:for-item="val" wx:for-index="_anonIdx6">{{item[val.escUserId] || 0}}</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
    </block>
</template>
<block wx:if="{{$taroCompReady}}">
    <view class="page report__root">
        <at-tabs compid="{{$compid__435}}"></at-tabs>
        <view class="report__control-wrap">
            <at-segmented-control compid="{{$compid__436}}"></at-segmented-control>
        </view>
        <view class="border-bottom report__pager">
            <pager compid="{{$compid__437}}"></pager>
        </view>
        <template is="renderMain" data="{{...anonymousState__temp}}"></template>
    </view>
</block>