<template name="renderMain">
    <block>
        <block wx:if="{{isFetching}}">
            <at-activity-indicator compid="{{$compid__396}}"></at-activity-indicator>
        </block>
        <block wx:elif="{{showType === 0}}">
            <view class="report__main">
                <tendency-chart compid="{{$compid__397}}"></tendency-chart>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-3">业务项</view>
                        <view class="at-col at-col-3">接单台数</view>
                        <view class="at-col at-col-3">成交台数</view>
                        <view class="at-col at-col-3">当前成交率</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item" wx:key="name" wx:for="{{$anonymousCallee__16}}" wx:for-item="item" wx:for-index="_anonIdx2">
                            <view class="at-col at-col-3">{{item.name}}</view>
                            <view class="at-col at-col-3">{{item.total}}</view>
                            <view class="at-col at-col-3">{{item.dealTotal}}</view>
                            <view class="at-col at-col-3">{{item.scale}}</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
        <block wx:elif="{{showType === 1}}">
            <view class="report__main">
                <statistical-chart compid="{{$compid__398}}"></statistical-chart>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-3">日期</view>
                        <view class="at-col at-col-3">接单台数</view>
                        <view class="at-col at-col-3">成交台数</view>
                        <view class="at-col at-col-3">成交率</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item" wx:for="{{loopArray53}}" wx:for-item="item" wx:for-index="_anonIdx4">
                            <view class="at-col at-col-3">{{item.$original.orderDate}}</view>
                            <view class="at-col at-col-3">{{item.$original.orderTotal || 0}}</view>
                            <view class="at-col at-col-3">{{item.$original.orderDealTotal || 0}}</view>
                            <view class="at-col at-col-3">{{item.$loopState__temp4}}</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
    </block>
</template>
<block wx:if="{{$taroCompReady}}">
    <view class="page report__root">
        <at-tabs compid="{{$compid__399}}"></at-tabs>
        <view class="report__control-wrap">
            <at-segmented-control compid="{{$compid__400}}"></at-segmented-control>
        </view>
        <view class="border-bottom report__pager">
            <pager compid="{{$compid__401}}"></pager>
        </view>
        <template is="renderMain" data="{{...anonymousState__temp5}}"></template>
    </view>
</block>