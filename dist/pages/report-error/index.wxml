<template name="renderMain">
    <block>
        <block wx:if="{{isFetching}}">
            <at-activity-indicator compid="{{$compid__358}}"></at-activity-indicator>
        </block>
        <block wx:elif="{{showType === 0}}">
            <view class="report__main">
                <tendency-chart compid="{{$compid__359}}"></tendency-chart>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-3">业务项</view>
                        <view class="at-col at-col-3">销售部</view>
                        <view class="at-col at-col-3">二手车部</view>
                        <view class="at-col at-col-3">总台数</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-3">{{data.orderTotal.name}}</view>
                            <view class="at-col at-col-3">{{data.orderTotal.xsTotal}}</view>
                            <view class="at-col at-col-3">{{data.orderTotal.escTotal}}</view>
                            <view class="at-col at-col-3">{{data.orderTotal.total}}</view>
                        </view>
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-3">{{data.timeoutTotal.name}}</view>
                            <view class="at-col at-col-3">{{data.timeoutTotal.xsTotal}}</view>
                            <view class="at-col at-col-3">{{data.timeoutTotal.escTotal}}</view>
                            <view class="at-col at-col-3">{{data.timeoutTotal.total}}</view>
                        </view>
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-3">{{data.timeoutRate.name}}</view>
                            <view class="at-col at-col-3">{{_$anonymousState__temp2}}</view>
                            <view class="at-col at-col-3">{{_$anonymousState__temp3}}</view>
                            <view class="at-col at-col-3">{{_$anonymousState__temp4}}</view>
                        </view>
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-3">{{data.timeoutDeal.name}}</view>
                            <view class="at-col at-col-3">{{data.timeoutDeal.timeoutDealTotal}}</view>
                            <view class="at-col at-col-3">-</view>
                            <view class="at-col at-col-3">{{data.timeoutDeal.timeoutDeal}}</view>
                        </view>
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-3">{{data.timeoutNoDeal.name}}</view>
                            <view class="at-col at-col-3">{{data.timeoutNoDeal.timeoutNoDealTotal}}</view>
                            <view class="at-col at-col-3">-</view>
                            <view class="at-col at-col-3">{{data.timeoutNoDeal.timeoutDeal}}</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
        <block wx:elif="{{showType === 1}}">
            <view class="report__main">
                <statistical-chart compid="{{$compid__360}}"></statistical-chart>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-2">日期</view>
                        <view class="at-col at-col-2 at-col--wrap">二手车评估</view>
                        <view class="at-col at-col-2 at-col--wrap">置换新车</view>
                        <view class="at-col at-col-2 at-col--wrap">再购新车</view>
                        <view class="at-col at-col-2 at-col--wrap">转介绍</view>
                        <view class="at-col at-col-2 at-col--wrap">超时总台数</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item" wx:for="{{$anonymousCallee__15}}" wx:for-item="item" wx:for-index="_anonIdx2">
                            <view class="at-col at-col-2 at-col--wrap">{{item.orderDate}}</view>
                            <view class="at-col at-col-2">{{item.escpgTotal || 0}}</view>
                            <view class="at-col at-col-2">{{item.zhxcTotal || 0}}</view>
                            <view class="at-col at-col-2">{{item.zgxcTotal || 0}}</view>
                            <view class="at-col at-col-2">{{item.zjsTotal || 0}}</view>
                            <view class="at-col at-col-2">{{item.total || 0}}</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
    </block>
</template>
<block wx:if="{{$taroCompReady}}">
    <view class="page report__root">
        <at-tabs compid="{{$compid__361}}"></at-tabs>
        <view class="report__control-wrap">
            <at-segmented-control compid="{{$compid__362}}"></at-segmented-control>
        </view>
        <view class="border-bottom report__pager">
            <pager compid="{{$compid__363}}"></pager>
        </view>
        <template is="renderMain" data="{{...anonymousState__temp6}}"></template>
    </view>
</block>