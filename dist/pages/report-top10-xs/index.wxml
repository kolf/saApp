<template name="renderMain">
    <block>
        <block wx:if="{{isFetching}}">
            <at-activity-indicator compid="{{$compid__438}}"></at-activity-indicator>
        </block>
        <block wx:elif="{{showType === 0}}">
            <view class="report__main">
                <view style="width:100%;height:320px">
                    <f-2-canvas compid="{{$compid__439}}"></f-2-canvas>
                </view>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-2">顾问</view>
                        <view class="at-col at-col-3">再购新车</view>
                        <view class="at-col at-col-2">置换新车</view>
                        <view class="at-col at-col-2">转介绍</view>
                        <view class="at-col at-col-3">完成总台数</view>
                    </view>
                    <view class="table-body">
                        <block wx:if="{{data}}">
                            <view class="at-row at-list__item" wx:key="name" wx:for="{{$anonymousCallee__25}}" wx:for-item="item" wx:for-index="_anonIdx2">
                                <view class="at-col at-col-2">{{item.name}}</view>
                                <view class="at-col at-col-3">{{item.gxcCount}}</view>
                                <view class="at-col at-col-2">{{item.zhxcCount}}</view>
                                <view class="at-col at-col-2">{{item.zjsCount}}</view>
                                <view class="at-col at-col-3">{{item.totalCount}}</view>
                            </view>
                        </block>
                    </view>
                </view>
            </view>
        </block>
        <block wx:elif="{{showType === 1}}">
            <view class="report__main">
                <view style="width:100%;height:320px">
                    <f-2-canvas compid="{{$compid__440}}"></f-2-canvas>
                </view>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-3">日期</view>
                        <view class="at-col at-col-3" wx:for="{{zheData}}" wx:for-item="item" wx:for-index="_anonIdx4">{{item.name}}</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item" wx:key="dateStr" wx:for="{{$anonymousCallee__27}}" wx:for-item="item" wx:for-index="_anonIdx8">
                            <view class="at-col at-col-3">{{item.dateStr}}</view>
                            <view class="at-col at-col-3" wx:for="{{zheData}}" wx:for-item="val" wx:for-index="_anonIdx6">{{item[val.userId] || 0}}</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
    </block>
</template>
<block wx:if="{{$taroCompReady}}">
    <view class="page report__root">
        <at-tabs compid="{{$compid__441}}"></at-tabs>
        <view class="report__control-wrap">
            <at-segmented-control compid="{{$compid__442}}"></at-segmented-control>
        </view>
        <view class="border-bottom report__pager">
            <pager compid="{{$compid__443}}"></pager>
        </view>
        <template is="renderMain" data="{{...anonymousState__temp}}"></template>
    </view>
</block>