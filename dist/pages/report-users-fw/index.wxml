<template name="renderMain">
    <block>
        <block wx:if="{{isFetching}}">
            <at-activity-indicator compid="{{$compid__376}}"></at-activity-indicator>
        </block>
        <block wx:elif="{{showType === 0}}">
            <view class="report__main">
                <tendency-chart compid="{{$compid__377}}"></tendency-chart>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-4">业务项</view>
                        <view class="at-col at-col-4">推荐客户量</view>
                        <view class="at-col at-col-4">业务占比</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-4">总计推荐</view>
                            <view class="at-col at-col-4">{{data.TOTAL.orderTypeTotal}}</view>
                            <view class="at-col at-col-4">---</view>
                        </view>
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-4">二手车评估</view>
                            <view class="at-col at-col-4">{{data.ESCPG.orderTypeTotal}}</view>
                            <view class="at-col at-col-4">{{data.ESCPG.proportion}}%</view>
                        </view>
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-4">再购新车</view>
                            <view class="at-col at-col-4">{{data.ZGXC.orderTypeTotal}}</view>
                            <view class="at-col at-col-4">{{data.ZGXC.proportion}}%</view>
                        </view>
                        <view class="at-row at-list__item">
                            <view class="at-col at-col-4">转介绍</view>
                            <view class="at-col at-col-4">{{data.ZJS.orderTypeTotal}}</view>
                            <view class="at-col at-col-4">{{data.ZJS.proportion}}%</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
        <block wx:elif="{{showType === 1}}">
            <view class="report__main">
                <statistical-chart compid="{{$compid__378}}"></statistical-chart>
                <view class="at-list  text-center">
                    <view class="at-row at-list__item table-head text-center bg-gray">
                        <view class="at-col at-col-3">日期</view>
                        <view class="at-col at-col-3 at-col--wrap">二手车评估</view>
                        <view class="at-col at-col-2">再购新车</view>
                        <view class="at-col at-col-2">转介绍</view>
                        <view class="at-col at-col-2">推荐总量</view>
                    </view>
                    <view class="table-body">
                        <view class="at-row at-list__item" wx:for="{{data}}" wx:for-item="d" wx:for-index="_anonIdx2">
                            <view class="at-col at-col-3">{{d.date}}</view>
                            <view class="at-col at-col-3">{{d.ESCTotal}}</view>
                            <view class="at-col at-col-2">{{d.ZGXCTotal}}</view>
                            <view class="at-col at-col-2">{{d.ZJSTotal}}</view>
                            <view class="at-col at-col-2">{{d.Total}}</view>
                        </view>
                    </view>
                </view>
            </view>
        </block>
    </block>
</template>
<block wx:if="{{$taroCompReady}}">
    <view class="page report__root">
        <at-tabs compid="{{$compid__379}}"></at-tabs>
        <view class="report__control-wrap">
            <at-segmented-control compid="{{$compid__380}}"></at-segmented-control>
        </view>
        <view class="border-bottom report__pager">
            <pager compid="{{$compid__381}}"></pager>
        </view>
        <template is="renderMain" data="{{...anonymousState__temp3}}"></template>
    </view>
</block>